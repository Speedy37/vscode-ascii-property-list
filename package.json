{
    "name": "ascii-plist",
    "displayName": "Old-Style ASCII Property Lists",
    "description": "Language support for Old-Style ASCII Property Lists",
    "version": "1.0.3",
    "publisher": "speedy37",
    "repository": {
        "type": "git",
        "url": "https://github.com/Speedy37/vscode-ascii-property-list.git"
    },
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "plist",
                "aliases": [
                    "ASCII Property list"
                ],
                "extensions": [
                    ".plist",
                    ".dict"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "plistex",
                "aliases": [
                    "ASCII Property list extended"
                ],
                "extensions": [],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "plist",
                "scopeName": "source.plist",
                "path": "./syntaxes/plist.tmLanguage.json"
            },
            {
                "language": "plistex",
                "scopeName": "source.plist.extended",
                "path": "./syntaxes/plist.extended.tmLanguage.json"
            }
        ]
    }
}