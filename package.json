{
    "name": "plist-v1",
    "displayName": "plist-v1",
    "description": "Language support for property list format",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "plist",
                "aliases": [
                    "Property list"
                ],
                "extensions": [
                    ".plist",
                    ".dict"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "plistex",
                "aliases": [
                    "Property list extended"
                ],
                "extensions": [],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "plist",
                "scopeName": "source.plist",
                "path": "./syntaxes/plist.tmLanguage.json"
            },
            {
                "language": "plistex",
                "scopeName": "source.plist.extended",
                "path": "./syntaxes/plist.extended.tmLanguage.json"
            }
        ]
    }
}